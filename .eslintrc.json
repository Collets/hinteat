{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "google",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "curly": [
            "error",
            "multi-or-nest"
        ]
    }
}